<?xml version="1.0" encoding="utf-8"?>
<Project>

	<PropertyGroup>
		<Authors>Philip Hendry</Authors>
		<Company>LlamaDash</Company>
		<Copyright>Copyright ï¿½ $(Company) $([System.DateTime]::Now.Year)</Copyright>
		<PackageIcon>Logo.png</PackageIcon>
